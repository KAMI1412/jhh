{
    "rewrites": [
        {
            "source": "/.well-known/farcaster.json",
            "destination": "/confessions-app/farcaster.json"
        },
        {
            "source": "/",
            "destination": "/confessions-app/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/confessions-app/$1"
        }
    ]
}